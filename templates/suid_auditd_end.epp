
rm -f /tmp/auditd.rules.suid
echo "---" > <%= $auditd_rules_fact_file %>
echo "security_baseline_auditd_rules:" >> <%= $auditd_rules_fact_file %>
uniq /tmp/auditd.rules.$$ | while read rule ; do
